<robot name = "robot">
    <link name="torso">
        <inertial>
            <origin xyz="0 -3 2.5" rpy="0 0 0"/>
            <mass value="10000.0" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="0 -3 2.5" rpy="0 0 0"/>
            <geometry>
                <box size="1 1 3" />
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 -3 2.5" rpy="0 0 0"/>
            <geometry>
                <box size="1 1 3" />
            </geometry>
        </collision>
    </link>
    <joint name="torso_arm" type="revolute">
       <parent link="torso"/>
       <child  link="arm"/>
       <origin rpy="0 0 0" xyz="0 -2.5 4" />
       <axis xyz="0 1 0"/>
       <limit effort="0.0" lower="-3.14159" upper="3.14159" velocity="0.0"/>
    </joint>
    <link name="arm">
        <inertial>
            <origin xyz="0 1.25 -0.16666666666666666" rpy="0 0 0"/>
            <mass value="1.0" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="0 1.25 -0.16666666666666666" rpy="0 0 0"/>
            <geometry>
                <box size="0.3333333333333333 2.5 0.3333333333333333" />
            </geometry>
            <material name="Tan">
                <color rgba="1.3 0.94 0.92 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 1.25 -0.16666666666666666" rpy="0 0 0"/>
            <geometry>
                <box size="0.3333333333333333 2.5 0.3333333333333333" />
            </geometry>
        </collision>
    </link>
    <joint name="arm_club" type="fixed">
       <parent link="arm"/>
       <child  link="club"/>
       <origin rpy="0 0 0" xyz="0 2.5 0" />
       <axis xyz="1 0 0"/>
       <limit effort="0.0" lower="-3.14159" upper="3.14159" velocity="0.0"/>
    </joint>
    <link name="club">
        <inertial>
            <origin xyz="0 0.16666666666666666 -1.5" rpy="0 0 0"/>
            <mass value="1.0" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="0 0.16666666666666666 -1.5" rpy="0 0 0"/>
            <geometry>
                <box size="0.3333333333333333 0.3333333333333333 2.8" />
            </geometry>
            <material name="Gray">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0.16666666666666666 -1.5" rpy="0 0 0"/>
            <geometry>
                <box size="0.3333333333333333 0.3333333333333333 2.8" />
            </geometry>
        </collision>
    </link>
</robot>